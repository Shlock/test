                    <li id="image-form-8-li"><div><label for="image-form-file-8"><img src="<?php echo e(asset('wap/community/client/images/addpic.png')); ?>"  id="img8"></label><a href="javascript:;" class="delete none" data-index="8"></a></div></li>
