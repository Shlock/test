                    var isCropper8 = 1;
                    var cropperMaxWidth8 = 640;
                    var imageFormFileHtml8 = '<input type="file" id="image-form-file-8" name="<?php echo e($image_form_args8['file_name']); ?>" style="border:none; background:transparent; width:1000px; height:500px;" accept="image/*" />';
                    jQuery(function($){
                        $(document).on("change", "#image-form-file-8", function(){
                            $.submitFormToIframe = function(type) {
                                $("#image-form-file-8").get(0).disabled = true;
                                $("#image-form-iscanvas-8").val(type);
                                $("#").show();
                                $("#image-form-iframe-8").one("load", function(){
                                    try{
